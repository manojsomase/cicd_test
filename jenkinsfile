node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'Default Maven';
    withSonarQubeEnv() {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=eduvanz-team_eduvanz-portal_AY832XY5fIF7viSqLnGB -Dsonar.projectName='eduvanz-portal'"
    }
  }
}